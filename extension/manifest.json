{
  "name": "Speedrundle",
  "description": "Speedrun your daily puzzle games",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Open side panel"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["sidePanel", "storage", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"]
}
